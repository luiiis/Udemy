/*
Operador de comparacion
*/

//operador igual (==)
console.log(3==3);//regresa un true
console.log(3=='3');//regresa true, pero javascript compara numero aunque sea un string

//operador que no es igual (!=)
console.log(3!=3);//false

//operador estrictamente igual (===)
console.log(3===3);//regresa un true

//operador desigualdad estricta (!==)
console.log(3!=='3');//regresa un true

//operadores >, >=, <, <=,
console.log(3>4);
console.log(3>=3);
console.log(2<4);
console.log(2<=2);